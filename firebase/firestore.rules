rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      allow read, write: if request.auth != null;
    }
    
    match /users/{document=**} {
    	allow read, write: if request.auth != null;
    }
    
    match /stripe_customers/{document=**} {
    	allow read, write;
    }
    
  }
}